{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/weeklyscore-server/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "tsConfigPath": "apps/weeklyscore-server/tsconfig.app.json"
  },
  "projects": {
    "authentication": {
      "type": "library",
      "root": "libs/authentication",
      "entryFile": "index",
      "sourceRoot": "libs/authentication/src",
      "compilerOptions": {
        "tsConfigPath": "libs/authentication/tsconfig.lib.json"
      }
    },
    "domain": {
      "type": "library",
      "root": "libs/domain",
      "entryFile": "index",
      "sourceRoot": "libs/domain/src",
      "compilerOptions": {
        "tsConfigPath": "libs/domain/tsconfig.lib.json"
      }
    },
    "data": {
      "type": "library",
      "root": "libs/data",
      "entryFile": "index",
      "sourceRoot": "libs/data/src",
      "compilerOptions": {
        "tsConfigPath": "libs/data/tsconfig.lib.json"
      }
    },
    "postgraphile": {
      "type": "library",
      "root": "libs/postgraphile",
      "entryFile": "index",
      "sourceRoot": "libs/postgraphile/src",
      "compilerOptions": {
        "tsConfigPath": "libs/postgraphile/tsconfig.lib.json"
      }
    },
    "weeklyscore-server": {
      "type": "application",
      "root": "apps/weeklyscore-server",
      "entryFile": "main",
      "sourceRoot": "apps/weeklyscore-server/src",
      "compilerOptions": {
        "tsConfigPath": "apps/weeklyscore-server/tsconfig.app.json"
      }
    },
    "app-consumer": {
      "type": "application",
      "root": "apps/app-consumer",
      "entryFile": "main",
      "sourceRoot": "apps/app-consumer/src",
      "compilerOptions": {
        "tsConfigPath": "apps/app-consumer/tsconfig.app.json"
      }
    }
  },
  "monorepo": true,
  "root": "apps/weeklyscore-server"
}
